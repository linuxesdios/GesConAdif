/* ==== Design Tokens (dark) ==== */
* { font-family: "Inter", "Segoe UI", "Helvetica Neue", Arial, sans-serif; font-size: 10.5pt; }
QWidget { background: #0B1220; color: #E5E7EB; } /* deep navy */
QToolTip { background: #111827; color: #F9FAFB; border-radius: 6px; padding: 6px 8px; }

/* Cards &amp; Grouping */
QGroupBox {
  border: 1px solid #1F2937; border-radius: 14px; margin-top: 18px;
  background: #0F172A; /* slate-900 */
}
QGroupBox::title {
  subcontrol-origin: margin; left: 12px; padding: 2px 10px;
  color: #F3F4F6; font-weight: 800;
}

/* Headings utility */
QLabel[role="heading"] { font-size: 14pt; font-weight: 900; letter-spacing: .2px; color: #FFFFFF; }

/* Tabs */
QTabWidget::pane { border: 1px solid #1F2937; border-radius: 14px; background: #0F172A; }
QTabBar::tab {
  padding: 10px 16px; margin-right: 6px; min-height: 30px;
  background: #111827; color: #E5E7EB;
  border: 1px solid #1F2937; border-bottom: none; border-top-left-radius: 12px; border-top-right-radius: 12px;
}
QTabBar::tab:selected { background: #0B1220; color: #FFFFFF; font-weight: 800; }
QTabBar::tab:hover { background: #0F172A; }

/* Inputs */
QLineEdit, QPlainTextEdit, QTextEdit, QSpinBox, QDoubleSpinBox, QDateEdit, QTimeEdit, QDateTimeEdit, QComboBox {
  border: 1px solid #273244; border-radius: 10px; padding: 8px 10px; background: #0B1220; color: #E5E7EB;
}
QLineEdit:focus, QPlainTextEdit:focus, QTextEdit:focus,
QSpinBox:focus, QDoubleSpinBox:focus, QDateEdit:focus, QTimeEdit:focus, QDateTimeEdit:focus, QComboBox:focus {
  border: 1px solid #22C55E;
  box-shadow: 0 0 0 2px rgba(34,197,94,0.22);
}

/* Checks &amp; Radios */
QCheckBox::indicator, QRadioButton::indicator { width: 18px; height: 18px; }
QCheckBox::indicator {
  border-radius: 5px; border: 1px solid #334155; background: #0B1220;
}
QCheckBox::indicator:checked { background: #22C55E; border: 1px solid #16A34A; }
QRadioButton::indicator {
  border-radius: 9px; border: 1px solid #334155; background: #0B1220;
}
QRadioButton::indicator:checked {
  background: radial-gradient(circle, #22C55E 0%, #22C55E 40%, transparent 45%);
  border: 1px solid #16A34A;
}

/* Buttons */
QPushButton, QToolButton {
  border: 1px solid #1F2937; border-radius: 12px; padding: 9px 16px;
  background: #0F172A; color: #E5E7EB; font-weight: 800;
}
QPushButton:hover, QToolButton:hover { background: #0B1220; border: 1px solid #22C55E; }
QPushButton:pressed, QToolButton:pressed { background: #09101C; }
QPushButton:disabled, QToolButton:disabled { color: #6B7280; background: #0B1220; border: 1px solid #1F2937; }
QPushButton#primaryButton { background: #22C55E; color: #062B16; border: 1px solid #16A34A; }
QPushButton#primaryButton:hover { background: #16A34A; color: #EFFFF6; }
QPushButton#dangerButton { background: #7F1D1D; color: #FECACA; border: 1px solid #DC2626; }
QPushButton#warningButton { background: #78350F; color: #FEF3C7; border: 1px solid #F59E0B; }
QPushButton#successButton { background: #065F46; color: #D1FAE5; border: 1px solid #10B981; }

/* Tables &amp; Trees */
QHeaderView::section {
  background: #111827; padding: 9px; border: 0; border-right: 1px solid #1F2937; font-weight: 900; color: #E5E7EB;
}
QTableView, QTreeView {
  gridline-color: #1F2937; selection-background-color: #064E3B; selection-color: #ECFDF5;
  alternate-background-color: #0F172A; border: 1px solid #1F2937; border-radius: 12px; background: #0B1220; color: #E5E7EB;
}
QTableView::item:selected, QTreeView::item:selected { background: #073B2F; color: #ECFDF5; }

/* Menus &amp; Bars */
QToolBar { background: #0F172A; border-bottom: 1px solid #1F2937; spacing: 8px; padding: 6px; }
QMenuBar { background: #0F172A; color: #E5E7EB; }
QMenuBar::item { padding: 6px 10px; }
QMenuBar::item:selected { background: #111827; border-radius: 6px; }
QMenu { background: #0F172A; border: 1px solid #1F2937; border-radius: 10px; color: #E5E7EB; }
QMenu::item { padding: 7px 14px; }
QMenu::item:selected { background: #0B1220; }

/* Scrollbars */
QScrollBar:vertical, QScrollBar:horizontal { background: #0F172A; border: none; border-radius: 6px; }
QScrollBar:vertical { width: 12px; } QScrollBar:horizontal { height: 12px; }
QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
  background: #334155; border-radius: 6px; min-height: 24px; min-width: 24px;
}
QScrollBar::handle:vertical:hover, QScrollBar::handle:horizontal:hover { background: #475569; }

/* Progress &amp; Slider */
QProgressBar { border: 1px solid #1F2937; border-radius: 12px; background: #0B1220; text-align: center; color: #E5E7EB; }
QProgressBar::chunk { background-color: #22C55E; border-radius: 12px; }
QSlider::groove:horizontal { border: 1px solid #1F2937; height: 6px; border-radius: 3px; background: #111827; }
QSlider::handle:horizontal { background: #22C55E; width: 18px; margin: -6px 0; border-radius: 9px; }

/* Splitter &amp; Status */
QSplitter::handle { background: #1F2937; width: 6px; height: 6px; }
QStatusBar { background: #0F172A; border-top: 1px solid #1F2937; color: #9CA3AF; }
</string></property>

/* ===== ADIF Modern UI Theme =====
   Palette (approx ADIF):
   - Primary Green: #006633
   - Dark Green:    #004225
   - Mid Green:     #0b7a44
   - Accent Lime:   #7ed957
   - Light Mint:    #e7f4ee
   - Soft Gray:     #f4f6f8
   - Text Dark:     #1f2a2e
   - Border:        #d5dbde
*/

/* Base window */
QMainWindow, QWidget {
    background: #f8fafb;
    color: #1f2a2e;
    font-family: &quot;Segoe UI&quot;, &quot;Roboto&quot;, &quot;Arial&quot;, sans-serif;
    font-size: 8.5pt;
}

/* Tabs */
QTabWidget::pane {
    border: 1px solid #d5dbde;
    background: #ffffff;
    border-radius: 14px;
    margin-top: 6px;
}
QTabBar::tab {
    background: #ffffff;
    color: #1f2a2e;
    padding: 8px 16px;
    margin-right: 4px;
    border: 1px solid #d5dbde;
    border-bottom: 2px solid transparent;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    min-width: 110px;
    font-weight: 600;
}
QTabBar::tab:selected {
    background: #e7f4ee;
    color: #004225;
    border: 1px solid #0b7a44;
    border-bottom: 2px solid #006633;
}
QTabBar::tab:hover:!selected {
    background: #f4f6f8;
}

/* Headings */
QLabel[heading=&quot;true&quot;] {
    background: qlineargradient(x1:0,y1:0,x2:1,y2:0,
        stop:0 #0b7a44, stop:1 #006633);
    color: #ffffff;
    border-radius: 10px;
    padding: 12px 18px;
    font-size: 16pt;
    font-weight: 700;
}

/* Labels general */
QLabel {
    color: #2c3e50;
    font-weight: 600;
    font-size: 9pt;
    background: transparent;
    padding: 4px 8px;
    border-radius: 6px;
}

/* Labels específicos para formularios */
QLabel:hover {
    background: rgba(236, 240, 241, 0.5);
}

/* Labels de encabezados importantes */
QLabel[heading=&quot;true&quot;] {
    background: qlineargradient(x1:0,y1:0,x2:1,y2:0,
        stop:0 #0b7a44, stop:1 #006633);
    color: #ffffff;
    border-radius: 10px;
    padding: 12px 18px;
    font-size: 16pt;
    font-weight: 700;
}

/* Labels para títulos de secciones */
QGroupBox::title {
    color: #2c3e50;
    font-weight: 700;
    font-size: 11pt;
    padding: 4px 12px;
    background: rgba(236, 240, 241, 0.8);
    border-radius: 8px;
    margin-left: 10px;
}

/* Estilos mejorados para QGroupBox */
QGroupBox {
    font-weight: 600;
    border: 2px solid #bdc3c7;
    border-radius: 12px;
    margin-top: 10px;
    padding-top: 15px;
    background: rgba(255, 255, 255, 0.95);
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 15px;
    padding: 6px 15px;
    background: qlineargradient(x1:0,y1:0,x2:1,y2:0, 
        stop:0 #ecf0f1, stop:1 #d5dbde);
    color: #2c3e50;
    border: 1px solid #bdc3c7;
    border-radius: 8px;
    font-size: 10pt;
    font-weight: 700;
}

/* Labels especiales con indicadores */
QLabel[money=&quot;true&quot;] {
    background: rgba(76, 175, 80, 0.1);
    color: #2E7D32;
    border-left: 4px solid #4CAF50;
    padding: 6px 12px;
    font-weight: 700;
}

QLabel[percentage=&quot;true&quot;] {
    background: rgba(33, 150, 243, 0.1);
    color: #1565C0;
    border-left: 4px solid #2196F3;
    padding: 6px 12px;
    font-weight: 700;
}

QLabel[info=&quot;true&quot;] {
    background: rgba(158, 158, 158, 0.1);
    color: #424242;
    border-left: 4px solid #757575;
    padding: 6px 12px;
    font-weight: 600;
}

/* Inputs */
QLineEdit, QTextEdit, QPlainTextEdit,
QSpinBox, QDoubleSpinBox, QDateEdit, QTimeEdit, QComboBox {
    background: #ffffff;
    border: 1px solid #d5dbde;
    border-radius: 10px;
    padding: 8px 10px;
    selection-background-color: #cfe9dd;
    selection-color: #1f2a2e;
}
QLineEdit:focus, QTextEdit:focus, QPlainTextEdit:focus,
QSpinBox:focus, QDoubleSpinBox:focus, QDateEdit:focus, QTimeEdit:focus, QComboBox:focus {
    border: 2px solid #0b7a44;
    background: #ffffff;
}

/* Spin arrows */
QSpinBox::up-button, QDoubleSpinBox::up-button,
QSpinBox::down-button, QDoubleSpinBox::down-button {
    background: #e7f4ee;
    border: 1px solid #d5dbde;
    border-radius: 6px;
    width: 22px;
    height: 18px;
    margin: 2px;
}
QSpinBox::up-button:hover, QDoubleSpinBox::up-button:hover,
QSpinBox::down-button:hover, QDoubleSpinBox::down-button:hover {
    background: #d7efe5;
}

/* Read-only look */
QLineEdit[readOnly=&quot;true&quot;], QTextEdit[readOnly=&quot;true&quot;],
QDoubleSpinBox[readOnly=&quot;true&quot;], QSpinBox[readOnly=&quot;true&quot;] {
    background: #f4f6f8;
    color: #55636a;
}

/* Buttons */
QPushButton {
    background: qlineargradient(x1:0,y1:0,x2:0,y2:1,
        stop:0 #0b7a44, stop:1 #006633);
    color: #ffffff;
    border: 1px solid #006633;
    border-radius: 12px;
    padding: 10px 16px;
    font-weight: 700;
}
QPushButton:hover {
    background: qlineargradient(x1:0,y1:0,x2:0,y2:1,
        stop:0 #0f8f52, stop:1 #0b7a44);
}
QPushButton:pressed {
    background: #004225;
}
QPushButton[destructive=&quot;true&quot;] {
    background: #b00020;
    border-color: #b00020;
}
QPushButton[secondary=&quot;true&quot;] {
    background: #ffffff;
    color: #0b7a44;
    border: 2px solid #0b7a44;
}

/* Tables */
QTableWidget, QTableView {
    background: #ffffff;
    alternate-background-color: #f8faf9;
    border: 1px solid #d5dbde;
    border-radius: 12px;
    gridline-color: #e3e7ea;
}
QHeaderView::section {
    background: #e7f4ee;
    color: #004225;
    padding: 8px;
    border: 1px solid #d5dbde;
    font-weight: 700;
}
QTableWidget::item:selected, QTableView::item:selected {
    background: #cfe9dd;
    color: #1f2a2e;
}

/* GroupBoxes */
QGroupBox {
    background: #ffffff;
    border: 1px solid #d5dbde;
    border-radius: 14px;
    margin-top: 16px;
    padding-top: 24px;
}
QGroupBox::title {
    subcontrol-origin: margin;
    left: 16px;
    padding: 6px 12px;
    background: #006633;
    color: #ffffff;
    border-radius: 8px;
    font-weight: 700;
}

/* Checkbox &amp; Radio */
QRadioButton, QCheckBox {
    spacing: 8px;
    font-weight: 600;
}
QRadioButton::indicator, QCheckBox::indicator {
    width: 20px; height: 20px;
}
QRadioButton::indicator:unchecked, QCheckBox::indicator:unchecked {
    border: 2px solid #0b7a44;
    border-radius: 4px;
    background: #ffffff;
}
QRadioButton::indicator:checked, QCheckBox::indicator:checked {
    border: 2px solid #006633;
    background: #0b7a44;
}

/* MDI area or containers */
QMdiArea {
    background: #f4f6f8;
    border: 1px solid #d5dbde;
    border-radius: 12px;
}

/* Subtle shadows on focusable containers (Qt style engines may ignore) */

/* ===== Ajustes extra para más viveza ===== */

/* Botones secundarios con degradado y azul suave */
QPushButton[secondary=&quot;true&quot;] {
    background: qlineargradient(x1:0,y1:0,x2:0,y2:1,
        stop:0 #ffffff, stop:1 #3a7ca5);
    color: #ffffff;
    border: 1px solid #3a7ca5;
}
QPushButton[secondary=&quot;true&quot;]:hover {
    background: qlineargradient(x1:0,y1:0,x2:0,y2:1,
        stop:0 #3a7ca5, stop:1 #0b7a44);
}

/* Tabs seleccionadas con acento azul */
QTabBar::tab:selected {
    background: #e7f4ee;
    color: #3a7ca5;
    border: 1px solid #3a7ca5;
    border-bottom: 2px solid #3a7ca5;
}

/* Encabezados de tabla con azul */
QHeaderView::section {
    background: #3a7ca5;
    color: #ffffff;
}

/* Bordes de inputs con azul al enfocar */
QLineEdit:focus, QTextEdit:focus, QPlainTextEdit:focus,
QSpinBox:focus, QDoubleSpinBox:focus, QDateEdit:focus, QTimeEdit:focus, QComboBox:focus {
    border: 2px solid #3a7ca5;
}

/* ===== Ajustes para más color ===== */

/* Botones principales con degradado azul-naranja */
QPushButton {
    background: qlineargradient(x1:0,y1:0,x2:1,y2:1,
        stop:0 #3a7ca5, stop:1 #ff8c42);
    color: #ffffff;
    border: 1px solid #3a7ca5;
    border-radius: 10px;
    padding: 8px 14px;
}
QPushButton:hover {
    background: qlineargradient(x1:0,y1:0,x2:1,y2:1,
        stop:0 #ff8c42, stop:1 #3a7ca5);
}

/* Encabezados con violeta */
QLabel[heading=&quot;true&quot;] {
    background: qlineargradient(x1:0,y1:0,x2:1,y2:0,
        stop:0 #8e44ad, stop:1 #3a7ca5);
    color: #ffffff;
    padding: 10px 16px;
    font-size: 12pt;
    border-radius: 8px;
}

/* Tabs con colores múltiples */
QTabBar::tab:selected {
    background: #7ed957;
    color: #000000;
    border: 1px solid #7ed957;
    border-bottom: 2px solid #ff8c42;
}
QTabBar::tab:hover:!selected {
    background: #ff8c42;
    color: #ffffff;
}

/* Inputs con borde naranja al enfocar */
QLineEdit:focus, QTextEdit:focus, QPlainTextEdit:focus,
QSpinBox:focus, QDoubleSpinBox:focus, QDateEdit:focus, QTimeEdit:focus, QComboBox:focus {
    border: 2px solid #ff8c42;
}

/* Encabezados de tabla con violeta */
QHeaderView::section {
    background: #8e44ad;
    color: #ffffff;
}
